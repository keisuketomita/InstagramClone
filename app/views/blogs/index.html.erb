<h1>投稿一覧</h1>
<p><%= notice %></p>
<table class="center-block">
  <% @blogs.each do |blog| %>
    <tr>
      <th>投稿内容</th>
      <th>ユーザー</th>
    </tr>
    <tr>
      <td><%= link_to blog.content, edit_blog_path(blog.id) %></td>
      <td>！ユーザーページへのリンク！</td>
    </tr>
  <% end %>
</table>

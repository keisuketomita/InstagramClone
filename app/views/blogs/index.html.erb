<h1>投稿一覧</h1>
<p><%= notice %></p>
<table class="center-block">
  <% @blogs.each do |blog| %>
    <tr>
      <th>投稿画像</th>
      <th>ユーザー</th>
    </tr>
    <tr>
      <td><%= link_to image_tag(blog.image.url, class: "image_tag"), edit_blog_path(blog.id) %></td>
      <td><%= link_to blog.user.name,user_path(blog.user.id) %></td>
    </tr>
  <% end %>
</table>
